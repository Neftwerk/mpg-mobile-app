{
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "endOfLine": "lf",
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^[./]",
        "^@/(.*)$",
        "^@assets/(.*)$",
        "^@components/(.*)$",
        "^@configs/(.*)$",
        "^@app/(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}