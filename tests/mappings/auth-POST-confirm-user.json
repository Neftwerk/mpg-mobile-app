{
	"request": {
		"method": "POST",
		"url": "/api/v1/auth/confirm-user",
		"bodyPatterns": [
			{
				"matchesJsonPath": "$[?(@.username == 'user@test.com' && @.code == '615278')]"
			}
		]
	},
	"response": {
		"status": 200,
		"headers": {
			"Content-Type": "application/json"
		},
		"jsonBody": {
			"data": {
				"type": "authentication",
				"attributes": {
					"success": true,
					"message": "User successfully confirmed"
				}
			},
			"links": {
				"self": "http://localhost:5000/api/v1/auth/confirm-user"
			}
		}
	}
}
